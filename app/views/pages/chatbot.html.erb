              <br>
              <div class="fol-dugme u--zoomIn" style="margin-top:15px; text-align: center;"><button style="width:30%; margin-top: 20px; text-align: center;" class="button_cta_normal" onclick="calculateSavings()">Calculate Savings</button> </div>
              <br>


            <script>
                function calculateSavings() {
                    // Get input values
                    var hoursPerWeek = parseFloat(document.getElementById('hoursPerWeek').value);
                    var costPerHour = parseFloat(document.getElementById('costPerHour').value);

                    // Calculate monthly savings
                    var hoursPerMonth = hoursPerWeek * 4;
                    var savings = hoursPerMonth * costPerHour;

                    // Display result
                    document.getElementById('result').innerHTML = '$' + savings.toFixed(2);
                }
            </script>
